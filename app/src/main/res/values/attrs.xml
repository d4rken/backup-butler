<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PreferenceView">
        <attr name="pvIcon" format="reference" />
        <attr name="pvTitle" format="reference|string" />
        <attr name="pvDescription" format="reference|string" />
    </declare-styleable>

    <declare-styleable name="SetupBarView">
        <attr name="sbvPositivePrimaryLabel" format="reference|string" />
        <attr name="sbvPositiveSecondaryLabel" format="reference|string" />
        <attr name="sbvNegativePrimaryLabel" format="reference|string" />
        <attr name="sbvNegativeSecondaryLabel" format="reference|string" />
    </declare-styleable>

    <declare-styleable name="RecyclerViewWrapperLayout">
        <attr name="rvwEmptyIcon" format="reference" />
        <attr name="rvwEmptyText" format="reference|string" />
        <attr name="rvwExplanationIcon" format="reference" />
        <attr name="rvwExplanationText" format="reference|string" />
        <attr name="rvwLoading" format="boolean" />
        <attr name="rvwLoadingUntilFirstChange" format="boolean" />
    </declare-styleable>
</resources>